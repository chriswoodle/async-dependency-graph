{
  "name": "async-dependency-graph",
  "version": "2.0.2",
  "description": "A dependency graph that can traverse asynchronous nodes.",
  "main": "dist/library.js",
  "types": "dist/library.d.js",
  "author": "Chris Woodle",
  "scripts": {
    "build": "tsc -b -v",
    "clean": "tsc -b --clean",
    "trace": "tsc --traceResolution",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chriswoodle/async-dependency-graph.git"
  },
  "license": "MIT",
  "keywords": [
    "graph",
    "async",
    "dependency",
    "tree"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^24.0.0",
    "jest": "^29.5.0",
    "jest-util": "^30.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^8.8.2",
    "typescript": "^5.7.3"
  },
  "packageManager": "yarn@4.12.0",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.spec.ts"
    ]
  },
  "publishConfig": {
    "provenance": true
  }
}
